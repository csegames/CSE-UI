{
  "private": true,
  "name": "cu-patcher-ui",
  "version": "0.1.0",
  "description": "Camelot Unchained Patcher UI",
  "license": "MPL-2.0",
  "main": "src/index.tsx",
  "author": "Unchained Entertainment, LLC <support@unchained-entertainment.com>",
  "contributors": [
    {
      "name": "Contributors",
      "url": "https://github.com/csegames/Camelot-Unchained/graphs/contributors"
    }
  ],
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-ui",
    "crafting"
  ],
  "files": [
    "dist/ui/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/Camelot-Unchained.git"
  },
  "scripts": {
    "prebuild": "rimraf dist/ui",
    "build": "webpack --mode production",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --mode development",
    "prettier": "prettier . --write",
    "gen": "npm run gen:api-gql && npm run gen:shard-gql && npm run gen:rest",
    "gen:api-gql": "download https://api.camelotunchained.com/v1/api/schema > ./src/api/schema.json && gql-gen --schema src/api/schema.json --template graphql-codegen-typescript-no-pascal-template --config ./gql-gen.json --out ./src/api/primary-graphql.ts",
    "gen:shard-gql": "download https://hatcheryapi.camelotunchained.com/v1/api/schema > ./src/api/schema.json && gql-gen --schema src/api/schema.json --template graphql-codegen-typescript-no-pascal-template --config ./gql-gen.json --out ./src/api/shard-graphql.ts",
    "gen:rest": "download https://hatcheryapi.camelotunchained.com/v1/codegen/definitions.ts?useUpdatedFormat=true > ./src/api/webapi.ts",
    "gen-local:api-gql": "download http://localhost:3337/v1/api/schema > ./src/api/schema.json && gql-gen --schema src/api/schema.json --template graphql-codegen-typescript-no-pascal-template --config ./gql-gen.json --out ./src/api/primary-graphql.ts",
    "gen-local:shard-gql": "download http://localhost:1337/v1/api/schema > ./src/api/schema.json && gql-gen --schema src/api/schema.json --template graphql-codegen-typescript-no-pascal-template --config ./gql-gen.json --out ./src/api/shard-graphql.ts",
    "gen-local:rest": "download http://localhost:1337/v1/codegen/definitions.ts?useUpdatedFormat=true > ./src/api/webapi.ts"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0-rc.1",
    "@csegames/linaria": "^0.1.103",
    "aphrodite": "^1.1.0",
    "classnames": "^2.2.6",
    "core-js": "^2.6.12",
    "cwd-in-node-modules": "^1.0.1",
    "emotion": "^9.2.3",
    "es6-promise": "^3.0.2",
    "eventemitter3": "^1.1.1",
    "graphql": "0.13.0",
    "moment": "^2.22.2",
    "normalizr": "^2.0.0",
    "raven-js": "^3.27.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-emotion": "9.2.3",
    "react-redux": "^5.0.5",
    "react-tap-event-plugin": "^3.0.2",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "redux-typed-modules": "^2.2.1",
    "reflux": "^0.3.0",
    "sass-importer-node": "^1.0.0"
  },
  "//": [
    "notes: ",
    "handlebars is a hidden requirement of graphql-codegen-typescript-no-pascal-template",
    "prettier must be locked to a specific version to prevent graphql-code-generator from crashing"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/plugin-syntax-jsx": "^7.0.0-rc.1",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-rc.1",
    "@babel/plugin-transform-react-jsx": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/graphql": "^0.13.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^6.0.0",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.9",
    "@types/react-transition-group": "^1.x",
    "apollo-codegen": "^0.20.2",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-graphql-tag": "^1.6.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "copy-webpack-plugin": "^4.5.2",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "download-cli": "^1.1.1",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-graphql": "^2.1.1",
    "file-loader": "^1.1.11",
    "graphql-code-generator": "^0.10.7",
    "graphql-codegen-typescript-no-pascal-template": "^0.10.10",
    "graphql-tag": "^2.9.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.0",
    "p-s": "^3.1.0",
    "prettier": "^2.7.1",
    "react-hot-loader": "^4.13.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "ts-loader": "^4.4.2",
    "ts-transform-graphql-tag": "^0.1.3",
    "tslint": "^5.4.3",
    "tslint-config-airbnb": "^5.3.0",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^4.9.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
