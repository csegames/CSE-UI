{
  "name": "@csegames/library",
  "version": "0.2.6",
  "description": "CSE UI Client Library",
  "license": "MPL-2.0",
  "main": "lib/camelotunchained/index.js",
  "typings": "lib/index.d.ts",
  "private": false,
  "author": "City State Entertainment, LLC <support@citystateentertainment.com>",
  "contributors": [
    "JB (codecorsair) <jb@codecorsair.com>",
    "Mehuge <mehuge@sorcerer.co.uk>",
    "saddieeiddas <saddie.eiddas@gmail.com>",
    "sYc0n <mathias.nachtmann@t-online.de>"
  ],
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-ui",
    "cu-core"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/camelot-unchained-client-library.git"
  },
  "watch": {
    "build": {
      "patterns": "src",
      "extensions": "ts,tsx,scss,html,jpg,png,webm,ogg,css,ico,js"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rootDir": "src"
  },
  "scripts": {
    "start": "nps",
    "prepare": "cwd-in-node-modules || (rimraf typings && typings install && nps gql.hordetest && nps definitions.hordetest)"
  },
  "dependencies": {
    "cwd-in-node-modules": "^1.0.1",
    "emotion": "^9.2.10",
    "es6-promise": "^3.0.2",
    "fuzzysearch": "^1.0.3",
    "google-protobuf": "^3.11.0",
    "graphql": "0.13.0",
    "graphql-tag": "^1.2.4",
    "node-xmpp-client": "2.1.0",
    "randomstring": "^1.1.5",
    "raven-js": "^3.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-emotion": "^9.2.10",
    "react-transition-group": "1.x",
    "redux-typed-modules": "^2.2.1"
  },
  "devDependencies": {
    "@csegames/linaria": "^0.1.101",
    "@types/graphql": "^0.13.4",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.109",
    "@types/randomstring": "^1.1.6",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^4.4.46",
    "@types/react-transition-group": "1.x",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "apollo-codegen": "^0.10.13",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "copyfiles": "^1.2.0",
    "download-cli": "^1.0.5",
    "envify": "^4.1.0",
    "graphql-code-generator": "^0.10.7",
    "graphql-codegen-typescript-no-pascal-template": "^0.10.10",
    "jest": "^19.0.2",
    "npm-watch": "^0.1.4",
    "nps": "^5.0.5",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^19.0.14",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^3.5.0",
    "typings": "^1.3.2"
  }
}
