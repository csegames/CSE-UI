{
  "private": true,
  "name": "cu-ui-hud",
  "version": "0.1.0",
  "description": "Camelot Unchained UI - hud",
  "license": "MPL-2.0",
  "main": "dist/hud.js",
  "author": "City State Entertainment, LLC <support@citystateentertainment.com>",
  "contributors": [
    "Ortu"
  ],
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-ui",
    "crafting"
  ],
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/cu-ui.git"
  },
  "watch": {
    "build": {
      "patterns": "src",
      "extensions": "ts,tsx"
    },
    "build:sass": {
      "patterns": "src",
      "extensions": "scss"
    },
    "copy:dist": {
      "patterns": "src",
      "extensions": "html,jpg,png,webm,ogg,svg,css,ico,js"
    },
    "browserify:lib": {
      "patterns": "../../library/lib",
      "extensions": "ts,js,css,scss"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "rootDir": "src"
  },
  "scripts": {
    "start": "nps",
    "postinstall": "rimraf typings && typings install"
  },
  "dependencies": {
    "@types/dragula": "^2.1.30",
    "aphrodite": "^1.1.0",
    "apisauce": "0.3.1",
    "apollo-client": "^0.8.4",
    "camelot-unchained": "^0.8.11",
    "classnames": "^2.2.5",
    "color-ops": "^2.0.0",
    "cu-xmpp-chat": "^0.3.0",
    "dragula": "^3.7.2",
    "es6-promise": "^3.2.1",
    "graphql-tag": "^1.2.4",
    "immutable": "^3.8.1",
    "jquery": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "node-xmpp-client": "^2.1.0",
    "object-assign-shim": "^1.0.0",
    "react": "^15.4.0",
    "react-apollo": "^0.10.0",
    "react-dom": "^15.4.0",
    "react-draggable": "^2.2.3",
    "react-dragula": "^1.1.17",
    "react-infinite": "^0.10.0",
    "react-infinite-scroller": "^1.0.8",
    "react-jss": "^5.3.0",
    "react-motion": "^0.4.4",
    "react-motion-slider": "^0.4.1",
    "react-redux": "^4.4.5",
    "react-resizable": "^1.6.0",
    "react-select": "^1.0.0-beta13",
    "react-slick": "^0.12.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "redux-typed-modules": "^2.2.1",
    "sass-importer-node": "^1.0.1",
    "whatwg-fetch": "^2.0.2",
    "yargs-parser": "^2.4.1"
  },
  "devDependencies": {
    "@types/aphrodite": "^0.5.5",
    "@types/classnames": "0.0.32",
    "@types/enzyme": "^2.8.0",
    "@types/isomorphic-fetch": "0.0.33",
    "@types/jest": "^19.2.2",
    "@types/lodash": "^4.14.52",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^15.0.10",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-dom": "^0.14.23",
    "@types/react-infinite": "0.0.29",
    "@types/react-redux": "^4.4.36",
    "@types/react-select": "^1.0.45",
    "@types/react-test-renderer": "^15.5.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/sinon": "^2.2.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "browserify-casesensitiverequire": "^1.0.1",
    "copyfiles": "^1.0.0",
    "envify": "^4.0.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "http-server": "^0.9.0",
    "jest": "^19.0.2",
    "livereload": "^0.4.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.8.0",
    "p-s": "^3.1.0",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0",
    "ts-jest": "^19.0.14",
    "tslint": "^5.0.0",
    "tslint-config-airbnb": "^1.1.1",
    "tslint-eslint-rules": "^4.0.0",
    "typescript": "^2.3.1",
    "typings": "^1.3.2",
    "watch-cli": "^0.2.1"
  }
}
